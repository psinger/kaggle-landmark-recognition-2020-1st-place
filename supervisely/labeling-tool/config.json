{
  "name": "ML Labeling Tool",
  "type": "app",
  "categories": [
    "labeling"
  ],
  "description": "use metric learning models to classify images",
  "docker_image": "supervisely/base-py-sdk:6.1.102",
  "main_script": "supervisely/labeling-tool/src/main.py",
  "gui_template": "supervisely/labeling-tool/src/gui.html",
  "task_location": "application_sessions",
  "isolate": true,
  "headless": false,
  "integrated_into": [
    "image_annotation_tool"
  ],
  "icon": "https://i.imgur.com/16BX0EP.png",
  "icon_background": "#FFFFFF",
  "hotkeys": [
    {
      "hotkey": "ctrl+]",
      "command": "next_object"
    },
    {
      "hotkey": "ctrl+[",
      "command": "prev_object"
    }
  ],
  "instance_version": "6.4.21"
}
