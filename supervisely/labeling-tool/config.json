{
  "name": "Metric Learning Labeling Tool",
  "type": "app",
  "categories": [
    "neural network",
    "images",
    "metric learning",
    "inference interfaces",
    "labeling"
  ],
  "description": "Use metric learning models to classify images",
  "docker_image": "supervisely/labeling:0.0.5",
  "instance_version": "6.4.57",
  "main_script": "supervisely/labeling-tool/src/main.py",
  "gui_template": "supervisely/labeling-tool/src/gui.html",
  "task_location": "application_sessions",
  "isolate": true,
  "headless": false,
  "integrated_into": [
    "image_annotation_tool"
  ],
  "icon": "https://i.imgur.com/16BX0EP.png",
  "icon_cover": true,
  "icon_background": "#FFFFFF",
  "hotkeys": [
    {
      "hotkey": "ctrl+]",
      "command": "next_object"
    },
    {
      "hotkey": "ctrl+[",
      "command": "prev_object"
    }
  ],
  "poster": "https://imgur.com/RdFfKO2.jpg"
}
